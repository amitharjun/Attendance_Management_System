<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.attendance_management_system">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Student_homepage"
            android:label="@string/title_activity_student_homepage"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".View_Studentattendance"
            android:label="@string/title_activity_view__studentattendance"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Faculty_homepage"
            android:label="@string/title_activity_faculty_homepage"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ViewClass"
            android:label="@string/title_activity_view_class"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login_Admin"
            android:label="@string/title_activity_login__admin"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login_Faculty"
            android:label="@string/title_activity_login__faculty"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Login_Student"
            android:label="@string/title_activity_login__student"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".select_Login" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>